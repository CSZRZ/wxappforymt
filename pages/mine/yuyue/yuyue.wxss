.tableView {
  display: flex;
  flex-direction: column;
}

.tableViewCell {
  display: flex;
  align-items: center;
  width: 100%;
  height: 200rpx;
  border-bottom: 1rpx solid #ebebeb;
}

.tableViewCell-image {
    width: 120rpx;
    height: 120rpx;
    margin: 0rpx 20rpx;
}

.tableViewCell-info {
  flex: 1;
  display: flex;
  flex-direction: column;
  margin: 20rpx 20rpx 20rpx 0rpx;
}

.inof-title {
  font-size: 16px;
}

.info-subtitle {
  font-size: 13px;
  padding: 10rpx 0rpx;
  color: #999999;
}

.info-time {
  font-size: 13px;
  color: #999999;
}